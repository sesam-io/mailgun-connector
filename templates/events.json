[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all",
    "namespaced_identifiers": false,
    "pump": {
      "rescan_cron_expression": "0 0 * * ?"
    },
    "source": {
      "dataset": "{{@ system @}}-domains-collect",
      "type": "dataset"
    },
    "transform": [
      {
        "operation": "{{@ datatype @}}-list",
        "side_effects": false,
        "system": "{{@ system @}}",
        "type": "rest"
      },
      {
        "rules": {
          "default": [
            [
              "create",
              [
                "apply",
                "{{@ datatype @}}",
                "_S.response.items"
              ]
            ],
            [
              "discard"
            ]
          ],
          "{{@ datatype @}}": [
            [
              "filter",
              [
                "is-not-empty",
                "_S.storage.key"
              ]
            ],
            [
              "copy",
              "*"
            ],
            [
              "add",
              "sesam_domainName",
              "_R._S.name"
            ],
            [
              "add",
              "_id",
              "_S.id"
            ]
          ]
        },
        "type": "dtl"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-all",
      "type": "dataset"
    },
    "transform": [
      {
        "properties": {
          "primary_key": "id"
        },
        "template": "transform-collect-rest",
        "type": "template"
      }
    ],
    "type": "pipe"
  }
]